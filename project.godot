; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

_global_script_classes=[{
"base": "Node",
"class": &"BaseComponent",
"language": &"GDScript",
"path": "res://guidot/base_component.gd"
}, {
"base": "Component",
"class": &"BaseRootComponent",
"language": &"GDScript",
"path": "res://guidot/base_root_component.gd"
}, {
"base": "BaseComponent",
"class": &"BasicComponent",
"language": &"GDScript",
"path": "res://guidot/basic_component.gd"
}, {
"base": "BaseComponent",
"class": &"Component",
"language": &"GDScript",
"path": "res://guidot/component.gd"
}, {
"base": "BaseRootComponent",
"class": &"RootComponent",
"language": &"GDScript",
"path": "res://guidot/root_component.gd"
}]
_global_script_class_icons={
"BaseComponent": "",
"BaseRootComponent": "",
"BasicComponent": "",
"Component": "",
"RootComponent": ""
}

[application]

run/main_scene="res://root.tscn"
config/features=PackedStringArray("4.0")
run/low_processor_mode=true

[autoload]

Utils="*res://guidot/utils.gd"
Gui="*res://guidot/gui.gd"
Guidot="*res://guidot/guidot.gd"

[display]

window/size/always_on_top=true
window/subwindows/embed_subwindows=false
